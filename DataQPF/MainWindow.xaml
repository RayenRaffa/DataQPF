<Window x:Class="DataQPF.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:DataQPF"
        mc:Ignorable="d"
        Title="DataQPF - Welcome" Height="auto" Width="auto">
    <Grid>
        <Grid.ColumnDefinitions>
            <ColumnDefinition Width="Auto" />
            <ColumnDefinition />
        </Grid.ColumnDefinitions>
        <Grid.RowDefinitions>
            <RowDefinition/>
            <RowDefinition/>
            <RowDefinition/>
            <RowDefinition/>
        </Grid.RowDefinitions>
        <Rectangle Name="SideMenu" Width="100" Grid.RowSpan="4" VerticalAlignment="Stretch" Fill="#99000000" />
        <Button Name="LoadButton" Grid.Row="4" Click="LoadButton_Click" Width="auto" Height="20">Load Data</Button>
    </Grid>

</Window>
